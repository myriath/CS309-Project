        <item name="colorTertiary">@color/tert</item>
        <item name="colorTertiaryContainer">@color/tertcont</item>

        <item name="colorOnPrimary">@color/primon</item>
        <item name="colorOnSecondary">@color/secoon</item>
        <item name="colorOnTertiary">@color/terton</item>

        <item name="colorOnPrimaryContainer">@color/primconton</item>
